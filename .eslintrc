{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"jest": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
    "ecmaVersion": 8,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"indent": [
			2,
			4,
			{
				"SwitchCase": 1
			}
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"no-unused-vars": 0,
		"no-console": 0
	},
	"globals": {
		"process": true,
		"_OUTFILES_": true,
		"__dirname": true,
		"_testColors": true,
		"Enzyme": true
	}
}